    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI::coordinator",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "api-implementation.src.mpic_coordinator_service.main:app",
                "--reload"
            ],
            "env": {
                "perspectives": "{\"PERSPECTIVE_NAME_CODE_STRING\": {\"caa_endpoint_info\": {\"url\": \"http://localhost:8082/caa\"}, \"dcv_endpoint_info\": {\"url\": \"http://localhost:8081/dcv\"}}}",
                "default_perspective_count": "1",
                "UVICORN_PORT": "8080",
                "available_perspectives": "{\"available_regions\": [{\"code\": \"PERSPECTIVE_NAME_CODE_STRING\", \"name\": \"Example Region\", \"rir\": \"arin\", \"too_close_codes\": []}]}"
            }
        },
        {
            "name": "Python Debugger: FastAPI::caa-checker",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "api-implementation.src.mpic_caa_checker_service.main:app",
                "--reload"
            ],
            "env": {
                "UVICORN_PORT": "8082"
            }
        },
        {
            "name": "Python Debugger: FastAPI::dcv-checker",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "api-implementation.src.mpic_dcv_checker_service.main:app",
                "--reload"
            ],
            "env": {
                "UVICORN_PORT": "8081"
            }
        }
    ]
}